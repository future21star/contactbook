<div id="success">
  <h1 style="color:red;">  Loading... </h1>
</div>

<script>
  $(document).ready(function(){
    $.ajax({
        type: "get",
        url: "/upload",
        data: {},
        success:function(data){
          $("#success").html(data);
          return false;
        },
        error:function(data) {
          alert(data);
          return false;
        },
        timeout: 1000 * 150 // sets timeout to 3 seconds
      });
  });
</script>